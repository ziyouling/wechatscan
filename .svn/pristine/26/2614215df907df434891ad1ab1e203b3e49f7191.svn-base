<Window x:Class="wechatscanWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wechatscanWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Left="980">
    <DockPanel>
        <StackPanel Orientation="Horizontal"  DockPanel.Dock="Top">
            <TextBlock Text="服务器:" />
            <TextBox Name="serverInput" MinWidth="100"  Text="https://course.muketang.com" />
            <Button Content="开始" Click="Button_Click" />
        </StackPanel>
        <TextBox Name="tb"  DockPanel.Dock="Top"/>
    </DockPanel>
</Window>
